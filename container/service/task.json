[{
    "name": "${name}",
    "image": "${repository}:${version}",
    "cpu": ${cpu},
    "memory": ${memory},
    "essential": true,
    "portMappings": [${port_mappings}],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group}",
            "awslogs-region": "${region}"
        }
    }
}]
